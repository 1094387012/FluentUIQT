FluDisplayCodeBox {
    background-color: rgb(247, 247, 247);
    border: 1px solid rgb(234, 234, 234);
    /* border: 1px solid rgb(1, 234, 234); */
    font-size: 14px;
    border-radius: 4px;
}

FluDisplayCodeBox[transparent=true] {
    background-color: transparent;
    font-size: 14px;
    border: none;
}