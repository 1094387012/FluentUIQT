FluCalendarSelectMonthView {
    background-color: rgb(31, 31, 31);
}

#label {
    color: black;
    /* width: 40px; */
    /* height: 40px; */
    border: 1px solid transparent;
    border-radius: 25px;
}

#label[outRange=true] {
    color: rgb(151, 151, 151);
}

#label[today=true] {
    color: black;
    background-color: rgb(0, 90, 158);
}

#label:hover {
    background-color: rgb(44, 44, 44);
}

#label:hover[today=true] {
    color: black;
    background-color: rgb(0, 90, 158);
}